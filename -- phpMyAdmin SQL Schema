--
-- Structure de la table `enchantments`
--

CREATE TABLE IF NOT EXISTS `enchantments` (
  `Rarity` text collate latin1_general_ci NOT NULL,
  `MinLevel` int(3) NOT NULL,
  `MaxLevel` int(3) NOT NULL,
  `Class` varchar(3) collate latin1_general_ci NOT NULL,
  `Modifiers` text collate latin1_general_ci NOT NULL,
  `MinMod` float NOT NULL,
  `MaxMod` float NOT NULL,
  `Probas` int(3) NOT NULL,
  `SlotRestrictions` text collate latin1_general_ci NOT NULL
) ENGINE=MyISAM DEFAULT CHARSET=latin1 COLLATE=latin1_general_ci;

