-- --------------------------------------------------------

--
-- Structure de la table `skills_talents`
--

CREATE TABLE IF NOT EXISTS `skills_talents` (
  `name` varchar(100) collate latin1_general_ci NOT NULL,
  `name_langid` varchar(100) collate latin1_general_ci NOT NULL,
  `desc_langid` varchar(100) collate latin1_general_ci NOT NULL,
  `class` varchar(3) collate latin1_general_ci NOT NULL,
  `otherfields` text collate latin1_general_ci NOT NULL,
  PRIMARY KEY  (`name`,`class`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 COLLATE=latin1_general_ci;

--
-- Contenu de la table `skills_talents`
--

