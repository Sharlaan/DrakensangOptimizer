CREATE TABLE IF NOT EXISTS `constants` (
  `level` int(2) NOT NULL,
  `class` varchar(3) collate latin1_general_ci NOT NULL,
  `basehp` int(4) NOT NULL,
  `basedamage` int(3) NOT NULL,
  `blockrate` float NOT NULL,
  `blockeddamage` float NOT NULL,
  `accr` float NOT NULL COMMENT 'accr : armor, critical chance, and resistance',
  PRIMARY KEY  (`level`,`class`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 COLLATE=latin1_general_ci;

