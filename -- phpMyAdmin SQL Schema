-- --------------------------------------------------------

--
-- Structure de la table `translations`
--

CREATE TABLE IF NOT EXISTS `translations` (
  `langid` varchar(100) collate latin1_general_ci NOT NULL,
  `french` text collate latin1_general_ci NOT NULL,
  `english` text collate latin1_general_ci NOT NULL,
  `spanish` text collate latin1_general_ci NOT NULL,
  `german` text collate latin1_general_ci NOT NULL,
  PRIMARY KEY  (`langid`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 COLLATE=latin1_general_ci;

--
-- Contenu de la table `translations`
--

