-- --------------------------------------------------------

--
-- Structure de la table `sets`
--

CREATE TABLE IF NOT EXISTS `sets` (
  `name` varchar(100) collate latin1_general_ci NOT NULL,
  `langid` varchar(100) collate latin1_general_ci NOT NULL,
  `class` varchar(3) collate latin1_general_ci NOT NULL,
  `pieces` text collate latin1_general_ci NOT NULL,
  `bonuses` text collate latin1_general_ci NOT NULL,
  PRIMARY KEY  (`name`,`class`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1 COLLATE=latin1_general_ci;

--
-- Contenu de la table `sets`
--

